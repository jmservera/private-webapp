{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "repo_owner": {
      "value": "${REPO_OWNER}"
    },
    "repo_name": {
      "value": "${REPO_NAME}"
    },
    "githubPAT": {
      "value": "${GITHUB_PAT}"
    },
    "githubToken": {
      "value": "${GITHUB_REPO_TOKEN}"
    },
    "publicKey": {
      "value": "${PUBLIC_KEY}"
    },
    "clientIpAddress": {
      "value": "${CLIENT_IP_ADDRESS}"
    },
    "private": {
      "value": "${PRIVATE}"
    }
  }
}